# Pyth Oracle Integration Environment Variables
# Copy this file to .env and fill in your values

# Ethereum RPC URL (Sepolia testnet recommended)
RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_PROJECT_ID

# Private key of your wallet (for testing only, use test wallet!)
PRIVATE_KEY=YOUR_PRIVATE_KEY_HERE

# Pyth Contract Address on Sepolia
PYTH_CONTRACT_ADDRESS=0x2880aB155794e7179c9eE2e38200202908C17B43

# API Server Port
API_PORT=3001

# Example values (DO NOT use in production):
# RPC_URL=https://sepolia.infura.io/v3/abc123...
# PRIVATE_KEY=0x1234567890abcdef...
# PYTH_CONTRACT_ADDRESS=0x2880aB155794e7179c9eE2e38200202908C17B43
# API_PORT=3001
